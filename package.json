{
  "author": "Justin Silver",
  "name": "developer-tools",
  "description": "Developer Tools provides Artillery, ESLint, Git precommit-hooks, Istanbul, and Mocha. NPM scripts for linting, validating modules, validating dependencies, code coverage (json, xunit, cobertura, lcov), and coverage badge.",
  "version": "0.0.15",
  "license": "MIT",
  "homepage": "https://github.com/doublesharp/node-developer-tools",
  "repository": {
    "url": "https://github.com/doublesharp/node-developer-tools.git"
  },
  "scripts": {
    "install": "node ./bin/install"
  },
  "bin": {
    "dependency-checker": "./bin/dependency-checker",
    "module-validator": "./bin/module-validator",
    "istanbul-coverage-report": "./bin/istanbul-coverage-report"
  },
  "dependencies": {
    "colors": "1.1.2",
    "depcheck": "0.6.7",
    "eslint": "3.13.0",
    "git-validate": "2.2.2",
    "istanbul": "0.4.5",
    "istanbul-cobertura-badger": "1.2.1",
    "mocha": "3.2.0",
    "nodemon": "1.11.0",
    "sloc": "0.1.11",
    "spec-xunit-file": "0.0.1-3"
  }
}
