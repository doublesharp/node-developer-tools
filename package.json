{
  "author": "Justin Silver",
  "name": "developer-tools",
  "description": "Developer Tools provides eslint, git precommit-hooks, nyc, and mocha. NPM scripts for linting, validating modules, validating dependencies, code coverage (json, xunit, cobertura, lcov), and coverage badge.",
  "version": "0.0.18",
  "license": "MIT",
  "homepage": "https://github.com/doublesharp/node-developer-tools",
  "repository": {
    "url": "https://github.com/doublesharp/node-developer-tools.git"
  },
  "scripts": {
    "install": "node ./bin/install"
  },
  "bin": {
    "dependency-checker": "./bin/dependency-checker",
    "module-validator": "./bin/module-validator",
    "istanbul-coverage-report": "./bin/istanbul-coverage-report"
  },
  "dependencies": {
    "colors": "1.4.0",
    "depcheck": "1.4.0",
    "eslint": "7.25.0",
    "git-validate": "2.2.4",
    "istanbul-cobertura-badger": "1.3.1",
    "mocha": "8.3.2",
    "nodemon": "2.0.7",
    "nyc": "15.1.0",
    "sloc": "0.2.1",
    "spec-xunit-file": "0.0.1-3"
  }
}
